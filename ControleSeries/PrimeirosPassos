-- Istalando pacotes composer.json
composer install

-- rodando migrate no banco de dados
php artisan migrate

-- subindo servidor
php artisan serve

-- Criando um modelo e uma migrate (Temporada)

php artisan make:model Temporada -m

-- Criando um modelo e uma migrate (Episodio)
php artisan make:model Episodio -m

-- Rodando migrate
php artisan migrate

-- Criando um Controller
php artisan make:controller Temporadas

-- Quando criar uma clase tem que rosa auto reload do composer
composer dump-autoload

--Criando um Controller
php artisan make:controller EpisodiosController

--adiconando uma migrate a uma tabela
php artisan make:migration adiciona_campo_assistido --table=episodios

-- Rodando migrate
php artisan migrate

-- Criando Estrutura de autenthicacao do laravel
php artisan make:auth
Depois rodar um
composer update

-- rota criada pelo laravel
http://localhost:8000/home

--criada por mim
http://localhost:8000/entrar

-- Criando Controller entrar
php artisan make:controller EntrarController
